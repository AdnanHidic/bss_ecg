<Window x:Class="Visualiser.Views.AnnotationDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AnnotationDelete" Height="366" Width="419">
    <Grid>
        <Label Content="Available annotations" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lb_annotations" HorizontalAlignment="Left" Height="290" Margin="10,36,0,0" VerticalAlignment="Top" Width="310"/>
        <Button x:Name="b_delete" Content="Delete" HorizontalAlignment="Left" Margin="325,36,0,0" VerticalAlignment="Top" Width="75" Click="b_delete_Click"/>
        <Button x:Name="b_close" Content="Close" HorizontalAlignment="Left" Margin="326,63,0,0" VerticalAlignment="Top" Width="75" Click="b_close_Click"/>

    </Grid>
</Window>
