<Window x:Class="Visualiser.Views.CalculateHROptions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CalculateHROptions" Height="151" Width="379">
    <Grid>
        <Label Content="Lower time index:" HorizontalAlignment="Left" Margin="11,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Upper time index:" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_lowerTimeIndex" HorizontalAlignment="Left" Height="23" Margin="119,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242"/>
        <TextBox x:Name="tb_upperTimeIndex" HorizontalAlignment="Left" Height="23" Margin="119,53,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242"/>
        <Button x:Name="b_ok" Content="OK" HorizontalAlignment="Left" Margin="200,89,0,0" VerticalAlignment="Top" Width="75" Click="b_ok_Click"/>
        <Button x:Name="b_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="286,89,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.4,0.5" Click="b_cancel_Click"/>

    </Grid>
</Window>
